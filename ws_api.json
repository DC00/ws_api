{
    "acceptedResourceRoles": [
        "slave_public"
    ],
    "container": {
        "docker": {
            "image": "mwhooker/ws_api:latest",
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 5002,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ]
        },
        "type": "DOCKER"
    },
    "cpus": 0.1,
    "env": {
        "REDIS_HOST": "mrredis.mesos",
        "REDIS_PORT": "5656"
    },
    "forcePullImage": true,
    "healthChecks": [
        {
            "gracePeriodSeconds": 5,
            "intervalSeconds": 20,
            "maxConsecutiveFailures": 3,
            "path": "/",
            "portIndex": 0,
            "protocol": "HTTP"
        }
    ],
    "id": "/counter",
    "instances": 1,
    "labels": {
        "DCOS_SERVICE_NAME": "counter",
        "DCOS_SERVICE_PORT_INDEX": "0",
        "DCOS_SERVICE_SCHEME": "http"
    },
    "mem": 64
}
